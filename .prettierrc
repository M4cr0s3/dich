{
    "singleQuote": true,
    "trailingComma": "all",
    "semi": true,
    "bracketSameLine": false,
    "bracketSpacing": true,
    "endOfLine": "lf",
    "jsxSingleQuote": true,
    "printWidth": 100,
    "quoteProps": "as-needed",
    "insertPragma": false,
    "useTabs": false,
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "strict",
    "vueIndentScriptAndStyle": true
}
